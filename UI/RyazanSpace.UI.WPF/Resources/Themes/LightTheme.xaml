    <SolidColorBrush x:Key="ButtonBorderMouseOver" Color="#FFFCB900" />
    <SolidColorBrush x:Key="ButtonBorderPressed" Color="#FFFCB900" />

    <SolidColorBrush x:Key="OptionMark.Static.Background" Color="#FFFFFFFF" />
    <SolidColorBrush x:Key="OptionMark.Static.Border" Color="#bbbbbb" />
    <SolidColorBrush x:Key="OptionMark.Static.Glyph" Color="#FF424242" />
    <SolidColorBrush x:Key="OptionMark.MouseOver.Background" Color="#FFFFFFFF" />
    <SolidColorBrush x:Key="OptionMark.MouseOver.Border" Color="#FFFCB900" />
    <SolidColorBrush x:Key="OptionMark.MouseOver.Glyph" Color="#FFFCB900" />
