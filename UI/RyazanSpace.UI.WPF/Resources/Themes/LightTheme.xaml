    <SolidColorBrush x:Key="TabControl.Header.Foreground.MouseOver" Color="#222222" />
    <SolidColorBrush x:Key="TabControl.Header.Foreground.Selected" Color="#222222" />
    <SolidColorBrush x:Key="TabControl.Header.Underline" Color="#FFBCBCBC" />
    <SolidColorBrush x:Key="TabControl.Header.Underline.MouseOver" Color="#FFDC082C" />
    <SolidColorBrush x:Key="TabControl.Header.Underline.Selected" Color="#FFFCB900" />

