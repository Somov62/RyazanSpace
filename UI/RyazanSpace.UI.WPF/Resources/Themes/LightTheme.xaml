<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="WindowBackground" Color="#FFEDEEF0" />
    <SolidColorBrush x:Key="WindowForeground" Color="#222222" />

    <SolidColorBrush x:Key="Accent" Color="#FFFCB900" />
    <SolidColorBrush x:Key="Primary" Color="#FFDC082C" />


    <SolidColorBrush x:Key="WindowNavBtnForeground" Color="Black" />
    <SolidColorBrush x:Key="WindowNavBtnBackground" Color="White" />
    <SolidColorBrush x:Key="WindowNavBtnBackgroundMouseOver" Color="#dddddd" />
    <SolidColorBrush x:Key="WindowNavBtnBackgroundPressed" Color="#bbbbbb" />


    <SolidColorBrush x:Key="TextBoxBackground" Color="#ffffff" />
    <SolidColorBrush x:Key="TextBoxForeground" Color="#222222" />
