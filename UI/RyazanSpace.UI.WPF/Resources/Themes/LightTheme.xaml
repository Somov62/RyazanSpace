    <SolidColorBrush x:Key="TextBoxCaret" Color="#FFDC082C" />
    <SolidColorBrush x:Key="TextBoxSelection" Color="#FFFCB900" />
    <SolidColorBrush x:Key="TextBoxBorder" Color="#bbbbbb" />
    <SolidColorBrush x:Key="TextBoxBorderMouseOver" Color="#FFFCB900" />

    <SolidColorBrush x:Key="ButtonForeground" Color="#fff" />
    <SolidColorBrush x:Key="ButtonBackground" Color="#FFFCB900" />
